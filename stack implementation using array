#include <stdio.h>
#include <stdlib.h>
#define max 110
int a[max];
int top=-1;
void push(int x)
{
    if(top==max-1)
        return("stack overflow");
    a[++top]=x;
}
void pop()
{
    if(top==-1)
        printf("stack underflow");
    top--;
}

void print()
{
    int i;
    for(i=0;i<=top;i++)
    {
        print("\n");

        printf("%d",a[i]);
    }
}
int main()
{
    push(2);print();
    push(3);print();
    push(4);print();
    pop();print();
    pop();print();
    pop();print();
    pop();
}
